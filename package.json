{
  "name": "@esnark/typeorm-mapped-types",
  "version": "0.3.1",
  "description": "Provide Mapped Types for typeorm",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "jest"
  },
  "keywords": [
    "typeorm",
    "nestjs"
  ],
  "author": "ESnark <settimeout1000@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@nestjs/mapped-types": "^2.0.4",
    "typeorm": "^0.3.20"
  },
  "peerDependencies": {
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@tsconfig/node16": "^16.1.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "reflect-metadata": "^0.2.0",
    "rimraf": "^5.0.5",
    "rxjs": "^7.1.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https//github.com/ESnark/typeorm-mapped-types.git"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.spec.ts"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "tsconfig": {
            "strict": false
          }
        }
      ]
    }
  },
  "homepage": "https://github.com/ESnark/typeorm-mapped-types",
  "packageManager": "yarn@4.6.0"
}
